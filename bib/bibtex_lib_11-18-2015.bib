@article{Byng:1994ws,
author = {Byng, J W and Boyd, N F and Fishell, E and Jong, R A and Yaffe, M J},
title = {{The quantitative analysis of mammographic densities.}},
journal = {Physics in medicine and biology},
year = {1994},
volume = {39},
number = {10},
pages = {1629--1638},
month = oct,
affiliation = {Department of Medical Biophysics and Radiology, University of Toronto and Imaging Research Group, Sunnybrook Health Science Centre, 2075 Bayview Avenue, Toronto, Ontario, Canada, M4N 3M5.},
pmid = { 15551535},
language = {English},
rating = {0},
date-added = {2012-07-25T02:15:50GMT},
date-modified = {2015-10-23T01:33:45GMT},
abstract = {Quantitative classification of mammographic parenchyma based on radiological assessment has been shown to provide one of the strongest estimates of the risk of developing breast cancer. Existing classification schemes, however, are limited by coarse category scales. In addition, subjectivity can lead to sizeable interobserver and intraobserver variations. Here, we propose an interactive thresholding technique applied to digitized film-screen mammograms, which assesses the proportion of the mammographic image representing radiographically dense tissue. Observers viewed images on a CRT display and selected grey-level thresholds from which the breast and regions of dense tissue in the breast were identified. The proportion of radiographic density was then calculated from the image histogram. The technique was evaluated for the mammograms of 30 women and is well correlated (R > 0.91, Spearman coefficient) with a six-category subjective classification of radiographic density by radiologists. The technique was found to be very reliable with an intraclass correlation coefficient between observers typically R > 0.9. This technique may have a role in routine mammographic analysis for the purpose of assessing risk categories and as a tool in studies of the etiology of breast cancer, in particular for monitoring changes in breast parenchyma during potential preventive interventions.},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15551535?dopt=Abstract},
local-url = {file://localhost/Users/fgimenez/Documents/Papers2/Articles/Byng/1994/Byng_The%20quantitative%20analysis%20of%20mammographic%20densities.pdf},
file = {{Byng_The quantitative analysis of mammographic densities.pdf:/Users/fgimenez/Documents/Papers2/Articles/Byng/1994/Byng_The quantitative analysis of mammographic densities.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/D7206074-58AF-4D70-953A-6EBD57FEE6BB}}
}

