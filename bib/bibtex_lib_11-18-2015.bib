@article{Do:2010hz,
author = {Do, Bao H and Wu, Andrew and Biswal, Sandip and Kamaya, Aya and Rubin, Daniel L},
title = {{Informatics in radiology: RADTF: a semantic search-enabled, natural language processor-generated radiology teaching file.}},
journal = {Radiographics : a review publication of the Radiological Society of North America, Inc},
year = {2010},
volume = {30},
number = {7},
pages = {2039--2048},
month = nov,
affiliation = {Department of Radiology, Stanford University Hospitals and Clinics, 300 Pasteur Dr, Room H1307, Stanford, CA 94305, USA. baodo@stanford.edu},
doi = {10.1148/rg.307105083},
pmid = {20801868},
language = {English},
rating = {0},
date-added = {2011-06-10T10:47:09GMT},
date-modified = {2015-11-19T03:34:51GMT},
abstract = {Storing and retrieving radiology cases is an important activity for education and clinical research, but this process can be time-consuming. In the process of structuring reports and images into organized teaching files, incidental pathologic conditions not pertinent to the primary teaching point can be omitted, as when a user saves images of an aortic dissection case but disregards the incidental osteoid osteoma. An alternate strategy for identifying teaching cases is text search of reports in radiology information systems (RIS), but retrieved reports are unstructured, teaching-related content is not highlighted, and patient identifying information is not removed. Furthermore, searching unstructured reports requires sophisticated retrieval methods to achieve useful results. An open-source, RadLex({\textregistered})-compatible teaching file solution called RADTF, which uses natural language processing (NLP) methods to process radiology reports, was developed to create a searchable teaching resource from the RIS and the picture archiving and communication system (PACS). The NLP system extracts and de-identifies teaching-relevant statements from full reports to generate a stand-alone database, thus converting existing RIS archives into an on-demand source of teaching material. Using RADTF, the authors generated a semantic search-enabled, Web-based radiology archive containing over 700,000 cases with millions of images. RADTF combines a compact representation of the teaching-relevant content in radiology reports and a versatile search engine with the scale of the entire RIS-PACS collection of case material.},
url = {http://radiographics.rsna.org/content/30/7/2039.abstract},
local-url = {file://localhost/Users/fgimenez/Documents/Papers2/Articles/Do/2010/Do_Informatics%20in%20Radiology%20RADTF%20A%20Semantic%20Search%E2%80%93enabled%20Natural%20Language%20Processor%E2%80%93generated%20Radiology%20Teaching%20File.pdf},
file = {{Do_Informatics in Radiology RADTF A Semantic Search–enabled Natural Language Processor–generated Radiology Teaching File.pdf:/Users/fgimenez/Documents/Papers2/Articles/Do/2010/Do_Informatics in Radiology RADTF A Semantic Search–enabled Natural Language Processor–generated Radiology Teaching File.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1148/rg.307105083}}
}

