@article{Gail:1989ui,
author = {Gail, Mitchell H and Brinton, Louise A and Byar, David P and Corle, Donald K and Green, Sylvan B and Schairer, Catherine and Mulvihill, John J},
title = {{Projecting Individualized Probabilities of Developing Breast Cancer for White Females Who Are Being Examined Annually}},
journal = {Journal of the National Cancer Institute},
year = {1989},
volume = {81},
number = {24},
pages = {1879--1886},
