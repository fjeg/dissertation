uri = {\url{papers2://publication/doi/10.1002/sim.4190}}
}

@article{Yokota:2004bn,
author = {Yokota, Fumie and Thompson, Kimberly M},
title = {{Value of Information Literature Analysis: A Review of Applications in Health Risk Management}},
journal = {Medical Decision Making},
year = {2004},
volume = {24},
