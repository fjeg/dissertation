uri = {\url{papers2://publication/uuid/10232428-2175-4E15-B449-1C14473154B2}}
}

@article{Bosmans:2011ep,
author = {Bosmans, Jan M. L. and Weyler, Joost J. and De Schepper, Arthur M. and Parizel, Paul M.},
title = {{The Radiology Report as Seen by Radiologists and Referring Clinicians: Results of the COVER and ROVER Surveys}},
journal = {Radiology},
year = {2011},
volume = {259},
number = {1},
pages = {184--195},
month = apr,
doi = {10.1148/radiol.10101045},
language = {English},
rating = {0},
date-added = {2011-07-14T20:39:05GMT},
date-modified = {2015-11-25T15:13:00GMT},
abstract = {Purpose: To investigate and compare the opinions and expectations regarding the radiology report of radiologists and referring clinicians and to identify trends, discordance, and discontent.Materials and Methods: A total of 3884 clinicians and 292 radiologists were invited by e-mail to participate in two internet surveys, COVER (for clinical specialists and general practitioners) and ROVER (for radiologists). Respondents were asked to state their level of agreement with 46 statements according to a Likert scale. Dichotomized results were compared by using the $\chi$2 statistic.Results: Eight hundred seventy-three completed forms were prepared for analysis, corresponding to a response rate of 21%. Most clinicians declared themselves satisfied with the radiology report. A large majority considered it an indispensable tool and accepted that the radiologist is the best person to interpret the images. Nearly all agreed that they need to provide adequate clinical information and state clearly what clinical question they want to have answered. Itemized reporting was preferred for complex examinations by both the clinicians and the radiologists. A majority in both groups were convinced that learning to report needs to be taught in a structured way.Conclusion: The surveys emphasize the role of the radiologist as a well-informed medical imaging specialist; however, some of the preferences of radiologists and clinicians diverge fundamentally from the way radiology is practiced and taught today, and implementing these preferences may have far-reaching consequences.{\copyright} RSNA, 2011},
url = {http://radiology.rsna.org/content/259/1/184.abstract},
local-url = {file://localhost/Users/fgimenez/Documents/Papers2/Articles/Bosmans/2011/Bosmans_The%20Radiology%20Report%20as%20Seen%20by%20Radiologists%20and%20Referring%20Clinicians%20Results%20of%20the%20COVER%20and%20ROVER%20Surveys.pdf},
file = {{Bosmans_The Radiology Report as Seen by Radiologists and Referring Clinicians Results of the COVER and ROVER Surveys.pdf:/Users/fgimenez/Documents/Papers2/Articles/Bosmans/2011/Bosmans_The Radiology Report as Seen by Radiologists and Referring Clinicians Results of the COVER and ROVER Surveys.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1148/radiol.10101045}}
}

