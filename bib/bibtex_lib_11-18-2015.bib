@article{Yousef:2009kf,
author = {Yousef, Waleed A and Kundu, Subrata and Wagner, Robert F},
title = {{Nonparametric estimation of the threshold at an operating point on the ROC curve}},
journal = {Computational Statistics {\&} Data Analysis},
year = {2009},
volume = {53},
number = {12},
pages = {4370--4383},
month = oct,
doi = {10.1016/j.csda.2009.06.006},
language = {English},
rating = {0},
date-added = {2013-08-06T06:43:32GMT},
date-modified = {2015-10-23T01:24:09GMT},
abstract = {In the problem of binary classification (or medical diagnosis), the classification rule (or diagnostic test) produces a continuous decision variable which is compared to a critical value (or threshold). Test values above (or below) that threshold are called positive (or negative) for disease. The two types of errors associated with every threshold value are Type I (false positive) and Type I I (false negative) errors. The Receiver Operating Curve (ROC) describes the relationship between probabilities of these two types of errors. The inverse problem is considered; i.e., given the ROC curve (or its estimate) of a particular classification rule, one is interested in finding the value of the threshold $\xi$ that leads to a specific operating point on that curve. A nonparametric method for estimating the threshold is proposed. Asymptotic distribution is derived for the proposed estimator. Results from simulated data and real-world data are presented for finite sample size. Finding a particular threshold value is crucial in medical diagnoses, among other fields, where a medical test is used to classify a patient as ``diseased'' or ``nondiseased'' based on comparing the test result to a particular threshold value. When the ROC is estimated, an operating point is obtained by fixing probability of one type of error, and obtaining the other one from the estimated curve. Threshold estimation can then be viewed as a quantile estimation for one distribution but with the utilization of the second one.},
url = {http://www.sciencedirect.com/science/article/pii/S0167947309002369},
local-url = {file://localhost/Users/fgimenez/Documents/Papers2/Articles/Yousef/2009/Yousef_Nonparametric%20estimation%20of%20the%20threshold%20at%20an%20operating%20point%20on%20the%20ROC%20curve.pdf},
file = {{Yousef_Nonparametric estimation of the threshold at an operating point on the ROC curve.pdf:/Users/fgimenez/Documents/Papers2/Articles/Yousef/2009/Yousef_Nonparametric estimation of the threshold at an operating point on the ROC curve.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.csda.2009.06.006}}
}

