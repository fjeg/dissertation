title = {{Differentially Expressed RNA from Public Microarray Data Identifies Serum Protein Biomarkers for Cross-Organ Transplant Rejection and Other Conditions}},
journal = {PLoS Comput Biol},
year = {2010},
volume = {6},
number = {9},
pages = {e1000940 EP --},
month = jan,
publisher = {Public Library of Science},
rating = {0},
date-added = {2011-06-01T21:51:00GMT},
date-modified = {2015-10-23T01:24:04GMT},
abstract = {<title>Author Summary</title> <p>Protein biomarkers in the blood are urgently needed for the diagnosis of a wide variety of diseases to improve health care. We aim to find a fast and cost-effective strategy to discover diagnostic protein biomarkers. Hundreds of diseases have already been investigated using microarray technology, measuring the mRNA expression of all genes in the disease-damaged tissues. We analyzed biopsy-based microarray data for 41 diseases in the public repository, identified genes with dysregulated mRNA expressions and detectable-protein abundance in the blood, and predicted them as candidate diagnostic protein biomarkers. We found that clinically and preclinically validated diagnostic protein biomarkers were significantly enriched in our predicted protein candidates for 22 diseases. We then measured the concentrations of ten predicted protein biomarkers in the serum samples from 39 renal transplant patients. Three of them were confirmed to be diagnostic of acute rejection after renal transplantation. All three proteins were further confirmed to be diagnostic of acute rejection in 63 cardiac transplant recipients. Our results show that publically available genome-wide gene expression data on disease-damaged tissues can be effectively translated into diagnostic protein biomarkers.</p>},
url = {http://dx.doi.org/10.1371%2Fjournal.pcbi.1000940},
local-url = {file://localhost/Users/fgimenez/Documents/Papers2/Articles/Chen/2010/Chen_Differentially%20Expressed%20RNA%20from%20Public%20Microarray%20Data%20Identifies%20Serum%20Protein%20Biomarkers%20for%20Cross-Organ%20Transplant%20Rejection%20and%20Other%20Conditions.pdf},
file = {{Chen_Differentially Expressed RNA from Public Microarray Data Identifies Serum Protein Biomarkers for Cross-Organ Transplant Rejection and Other Conditions.pdf:/Users/fgimenez/Documents/Papers2/Articles/Chen/2010/Chen_Differentially Expressed RNA from Public Microarray Data Identifies Serum Protein Biomarkers for Cross-Organ Transplant Rejection and Other Conditions.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/9261547B-25B2-491F-9015-A2059E3472AA}}
}

